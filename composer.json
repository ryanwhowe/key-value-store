{
    "name": "RyanWHowe/keyvaluestore",
    "description": "A Key/Value storage setter and getter library with a database backend",
    "type": "library",
    "require": {
        "php": ">=5.6",
        "doctrine/dbal": "^2.5",
        "ext-pdo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "ext-sqlite3": "*",
        "symfony/var-dumper": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "RyanWHowe\\KeyValueStore\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan W. Howe",
            "email": "RyanWHowe@gmail.com"
        }
    ],
    "minimum-stability": "stable"
}
